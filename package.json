{
  "name": "password-backpack",
  "description": "Secure password manager",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Marcin Ko≈Çodziejczak",
    "email": "kolodziejczak.mn@gmail.com"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "sweetalert": "^2.1.0",
    "uuid": "^3.2.1"
  },
  "homepage": "./",
  "main": "build/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint ./",
    "prebuild": "npm run lint",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 electron public/electron.js",
    "start-dev": "concurrently 'npm start' 'npm run electron-dev'",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-packager . --platform linux --arch x64 --out dist/",
    "deb64": "electron-installer-debian --src dist/password-backpack-linux-x64/ --dest dist/installers/ --arch amd64 --description Secure password manager"
  },
  "pre-push": [
    "lint"
  ],
  "devDependencies": {
    "concurrently": "^3.5.1",
    "electron": "^1.8.2",
    "electron-installer-debian": "^0.8.1",
    "electron-packager": "^11.0.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "pre-push": "^0.1.1"
  }
